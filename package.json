{
  "name": "@artesa/vue-pdf-embed",
  "version": "1.1.6",
  "description": "PDF embed component for Vue 2 and Vue 3",
  "keywords": [
    "vue",
    "vuejs",
    "pdf"
  ],
  "main": "./dist/vue-pdf-embed.umd.js",
  "module": "./dist/vue-pdf-embed.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-pdf-embed.es.js",
      "require": "./dist/vue-pdf-embed.umd.js"
    },
    "./style": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "types": "types/vue3-pdf-embed.d.ts",
  "files": [
    "dist/*",
    "src/*",
    "types/*"
  ],
  "license": "MIT",
  "author": "Aliaksei Hrynko (https://github.com/hrynko)",
  "repository": "github:Artesa/vue-pdf-embed",
  "scripts": {
    "prepare": "vite build",
    "build": "vite build",
    "serve": "vite serve -c vite.demo.config.js demo",
    "test": "",
    "lint": "eslint \"**/*.{js,vue}\"",
    "lint:fix": "eslint \"**/*.{js,vue}\" --fix"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.10.0",
    "pdfjs-dist": "^2.9.359",
    "prettier": "^2.3.0",
    "sass": "^1.57.1",
    "vite": "^4.0.4",
    "vue": "^3.2.45"
  },
  "peerDependencies": {
    "vue": "^3.x"
  }
}
